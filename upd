# /bin/sh

sudo cp -p PdfDoc.py /Library/Python/2.7/site-packages/.
sudo cp -p PdfFontMetrics.py /Library/Python/2.7/site-packages/.
sudo cp -p pdfFontMetrics.json /Library/Python/2.7/site-packages/.

sudo cp -p PdfDoc.py /Library/Frameworks/Python.framework/Versions/3.8/lib/python3.8/site-packages/
sudo cp -p PdfFontMetrics.py /Library/Frameworks/Python.framework/Versions/3.8/lib/python3.8/site-packages/.
sudo cp -p PdfFontMetrics.json  /Library/Frameworks/Python.framework/Versions/3.8/lib/python3.8/site-packages/.
